        },
        "Env": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "TERM": {
                    "type": "string",
                    "title": "TERM Environment variable",
                    "description": "This value is used to set the `$TERM` environment variable for each instance of Alacritty.\nIf it is not present, alacritty will check the local terminfo database and use `alacritty` if it is available, otherwise `xterm-256color` is used.\n(.bashrc will disable colors if it does not recognize $TERM)"
                }
            },
            "required": [],
            "title": "Env",
            "description": "Any items in the `env` entry below will be added as environment variables. Some entries may override variables set by alacritty it self."
        },
        "Font": {
