{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/AlacrittyConfig",
    "definitions": {
        "AlacrittyConfig": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "import": {
                    "type": "array",
                    "items": {},
                    "title": "Import additional configuration files",
                    "description": "Imports are loaded in order, skipping all missing files, with the importing file being loaded last. If a field is already present in a previous import, it will be replaced.\nAll imports must either be absolute paths starting with `/`, or paths relative to the user's home directory starting with `~/`."
                },
                "env": {
                    "$ref": "#/definitions/Env"
                },
                "window": {
                    "$ref": "#/definitions/Window"
                },
                "scrolling": {
                    "$ref": "#/definitions/Scrolling"
                },
                "font": {
                    "$ref": "#/definitions/Font"
                },
                "draw_bold_text_with_bright_colors": {
                    "type": "boolean"
                },
                "colors": {
                    "$ref": "#/definitions/Colors"
                },
                "bell": {
                    "$ref": "#/definitions/Bell"
                },
                "selection": {
                    "$ref": "#/definitions/Selection"
                },
                "cursor": {
                    "$ref": "#/definitions/Cursor"
                },
                "live_config_reload": {
                    "type": "boolean"
                },
                "shell": {
                    "type": "null"
                },
                "working_directory": {
                    "type": "string"
                },
                "alt_send_esc": {
                    "type": "boolean"
                },
                "ipc_socket": {
                    "type": "boolean"
                },
                "mouse": {
                    "$ref": "#/definitions/Mouse"
                },
                "hints": {
                    "$ref": "#/definitions/WelcomeHints"
                },
                "mouse_bindings": {
                    "type": "null"
                },
                "key_bindings": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/KeyBinding"
                    }
                },
                "debug": {
                    "$ref": "#/definitions/Debug"
                }
            },
            "required": [],
            "title": "Welcome"
        },
        "Bell": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "animation": {
